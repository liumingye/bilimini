{
  "name": "webmini",
  "version": "1.0.8-beta.1",
  "main": "dist/main/index.cjs",
  "author": "liumingye <liumingye2015@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/liumingye/webmini.git"
  },
  "bugs": {
    "url": "https://github.com/liumingye/webmini/issues"
  },
  "homepage": "https://github.com/liumingye/webmini",
  "packageManager": "yarn@3.2.0",
  "scripts": {
    "dev": "node scripts/watch.mjs",
    "format": "prettier --write ./packages",
    "stylelint": "stylelint --fix \"packages/**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "prebuild": "node scripts/build.mjs",
    "build": "yarn prebuild && electron-builder",
    "build:mac": "yarn build -m",
    "build:win": "yarn build -w",
    "build:linux": "yarn build -l",
    "lint:prettier": "prettier --check ./packages",
    "lint:eslint": "eslint --cache --cache-location node_modules/.cache/eslint/ --max-warnings 0 ./packages --ext .ts,.tsx,.vue --fix",
    "release": "yarn build --publish always",
    "electron-fix": "yarn dlx electron-fix start"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "dependencies": {
    "@cliqz/adblocker-electron": "^1.23.7",
    "@electron/remote": "^2.0.8",
    "axios": "^0.26.1",
    "electron-is": "^3.0.0",
    "lodash": "^4.17.21",
    "pouchdb": "^7.2.2",
    "tar": "^6.1.11",
    "vue-request": "^1.2.4",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.6.1"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.22.1",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/lodash": "^4.14.181",
    "@types/nprogress": "^0.2.0",
    "@types/overlayscrollbars": "^1.12.1",
    "@types/pouchdb": "^6.4.0",
    "@types/tar": "^6.1.1",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.9",
    "autoprefixer": "^10.4.4",
    "electron": "^17.4.0",
    "electron-builder": "^23.0.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-fetch": "^1.8.0",
    "electron-updater": "^4.6.5",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.6.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "nprogress": "^0.2.0",
    "overlayscrollbars": "^1.13.1",
    "pinia": "^2.0.13",
    "postcss": "^8.4.12",
    "postcss-html": "^1.3.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.6.2",
    "stylelint": "^14.6.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.6.3",
    "unplugin-auto-import": "^0.6.9",
    "unplugin-vue-components": "^0.18.5",
    "vite": "^2.9.1",
    "vite-plugin-optimize-persist": "^0.1.2",
    "vite-plugin-package-config": "^0.1.1",
    "vite-plugin-windicss": "^1.8.3",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14",
    "windicss": "^3.5.1"
  },
  "electronLanguagesInfoPlistStrings": {
    "en": {
      "CFBundleDisplayName": "WebMini",
      "CFBundleName": "WebMini"
    },
    "zh_CN": {
      "CFBundleDisplayName": "WebMini",
      "CFBundleName": "WebMini"
    }
  },
  "build": {
    "appId": "org.electron.webmini",
    "productName": "webmini",
    "asar": true,
    "artifactName": "${name}-${version}-${os}-${arch}.${ext}",
    "directories": {
      "output": "release/${version}",
      "buildResources": "build/icons"
    },
    "files": [
      "dist",
      "resources/**/*"
    ],
    "afterPack": "./build/buildAfterPack.js",
    "linux": {
      "category": "Network",
      "target": [
        "appimage",
        "snap"
      ]
    },
    "mac": {
      "category": "public.app-category.navigation",
      "extendInfo": {
        "LSHasLocalizedDisplayName": true
      },
      "target": [
        {
          "target": "zip",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "darkModeSupport": true
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "snap": {
      "publish": [
        {
          "provider": "github"
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true
    },
    "electronDownload": {
      "mirror": "https://npmmirror.com/mirrors/electron/"
    }
  },
  "env": {
    "//": "Used in build scripts",
    "PORT": 3344
  },
  "keywords": [
    "bilibili",
    "video",
    "vite",
    "electron",
    "vue3",
    "rollup"
  ],
  "vite": {
    "optimizeDeps": {
      "include": [
        "@arco-design/web-vue",
        "@arco-design/web-vue/es/icon",
        "@electron/remote",
        "axios",
        "electron",
        "electron-is",
        "less",
        "lodash",
        "nprogress",
        "overlayscrollbars",
        "pinia",
        "vue",
        "vue-request",
        "vue-router",
        "winston",
        "winston-daily-rotate-file"
      ]
    }
  }
}
