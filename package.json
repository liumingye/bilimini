    "electron": "16.0.8",
    "electron-builder": "^22.14.5",
    "less": "^4.1.2",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.11",
    "postcss-html": "^1.3.0",
    "postcss-less": "^6.0.0",
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
    "vite-plugin-resolve": "^1.4.3",
