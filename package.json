{
  "name": "bilimini",
  "version": "1.0.4-beta.1",
  "main": "dist/main/index.cjs",
  "author": "liumingye <liumingye2015@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/liumingye/bilimini.git"
  },
  "bugs": {
    "url": "https://github.com/liumingye/bilimini/issues"
  },
  "homepage": "https://github.com/liumingye/bilimini",
  "packageManager": "yarn@3.1.1",
  "scripts": {
    "dev": "node scripts/watch.mjs",
    "format": "prettier --write ./packages",
    "stylelint": "stylelint --fix \"packages/**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "prebuild": "node scripts/build.mjs",
    "build": "yarn prebuild && electron-builder",
    "build:mac": "yarn build -m",
    "build:win": "yarn build -w",
    "build:linux": "yarn build -l",
    "lint:prettier": "prettier --check ./packages",
    "lint:eslint": "eslint --cache --cache-location node_modules/.cache/eslint/ --max-warnings 0 ./packages --ext .ts,.tsx,.vue --fix",
    "release": "yarn build --publish always"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "dependencies": {
    "@electron/remote": "^2.0.5",
    "@cliqz/adblocker-electron": "^1.23.6",
    "lodash": "^4.17.21",
    "winston": "^3.6.0",
    "winston-daily-rotate-file": "^4.6.1"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.18.0",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/electron-json-storage": "^4.5.0",
    "@types/lodash": "^4.14.179",
    "@types/nprogress": "^0.2.0",
    "@types/overlayscrollbars": "^1.12.1",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vitejs/plugin-vue-jsx": "^1.3.8",
    "autoprefixer": "^10.4.2",
    "electron": "17.1.0",
    "electron-builder": "^22.14.13",
    "electron-devtools-installer": "^3.2.0",
    "electron-fetch": "^1.7.4",
    "electron-is": "^3.0.0",
    "electron-json-storage": "^4.5.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "nprogress": "^0.2.0",
    "overlayscrollbars": "^1.13.1",
    "pinia": "^2.0.11",
    "postcss": "^8.4.7",
    "postcss-html": "^1.3.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.5.1",
    "stylelint": "^14.5.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-vue": "^1.3.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.6.0",
    "unplugin-auto-import": "^0.6.1",
    "unplugin-vue-components": "^0.17.21",
    "vite": "^2.8.6",
    "vite-plugin-optimize-persist": "^0.1.2",
    "vite-plugin-package-config": "^0.1.1",
    "vite-plugin-windicss": "^1.8.3",
    "vue": "^3.2.31",
    "vue-router": "^4.0.13",
    "windicss": "^3.5.1"
  },
  "env": {
    "//": "Used in build scripts",
    "PORT": 3344
  },
  "keywords": [
    "bilibili",
    "video",
    "vite",
    "electron",
    "vue3",
    "rollup"
  ],
  "vite": {
    "optimizeDeps": {
      "include": [
        "@arco-design/web-vue",
        "less",
        "lodash",
        "nprogress",
        "overlayscrollbars",
        "pinia",
        "vue",
        "vue-router"
      ]
    }
  }
}
